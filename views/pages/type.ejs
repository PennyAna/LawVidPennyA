<!DOCTYPE html>
<div class="container">
    <h2>Type Results</h2>
    Hello there! I access the postgresql database for this app!<br>
    <ul>
            <% tv.forEach(function(r) { %>
                <li><%= r.title_id %> - 
                    <%= r.title_name %></li>
            <% }); %>
            <% film.forEach(function(s) { %>
                <li><%= s.title_id %> - 
                    <%= s.title_name %></li>
            <% }); %>
            <% other.forEach(function(t) { %>
                <li><%= t.title_id %> - 
                    <%= t.title_name %></li>
            <% }); %> 
    </ul>
</div>